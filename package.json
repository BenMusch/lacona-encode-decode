{
  "name": "lacona-encode-decode",
  "version": "1.0.0",
  "description": "Lacona Addon to Encode & decode text from multiple formats (url encoding, utf-8, etc.)",
  "main": "lib/index.js",
  "lacona": {
    "title": "Encode/Decode",
    "description": "Encode & decode text from multiple formats (url encoding, utf-8, etc.)",
    "config": "config.json"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "lacona",
    "lacona-addon"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BenMusch/lacona-encode-decode"
  },
  "dependencies": {
    "lacona-phrases": "^1.0.0",
    "lacona-api": "^1.0.0",
    "elliptical": "^1.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-cli": "^6.0.0",
    "rimraf": "^2.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
